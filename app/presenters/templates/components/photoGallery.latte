	<a href="/images/gallery/all/{block filename}{$photoRow->id}.{$photoRow->fileextension}{/block}" target="_blank" class="phgal" data-xlsize="{$photoRow->width}x{$photoRow->height}" data-lsize="{$photoRow->l_width}x{$photoRow->l_height}" data-msize="{$photoRow->m_width}x{$photoRow->m_height}" data-ssize="{$photoRow->s_width}x{$photoRow->s_height}" data-sizecat="xl" data-filename="{include filename}">
		<li>
			{include 'photoMiniatur.latte', row => $photoRow, posunXY => ano}
		</li>
	</a>
